<p id="notice"><%= notice %></p>

<h1>Party Relationship Types</h1>

<table>
  <thead>
    <tr>
      <th>From role</th>
      <th>To role</th>
      <th>Description</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @party_relationship_types.each do |party_relationship_type| %>
      <tr>
        <td><%= party_relationship_type.from_role_id %></td>
        <td><%= party_relationship_type.to_role_id %></td>
        <td><%= party_relationship_type.description %></td>
        <td><%= party_relationship_type.name %></td>
        <td><%= link_to 'Show', party_relationship_type %></td>
        <td><%= link_to 'Edit', edit_party_relationship_type_path(party_relationship_type) %></td>
        <td><%= link_to 'Destroy', party_relationship_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Party Relationship Type', new_party_relationship_type_path %>
