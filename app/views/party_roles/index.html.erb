<p id="notice"><%= notice %></p>

<h1>Party Roles</h1>

<table>
  <thead>
    <tr>
      <th>Party</th>
      <th>Role type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @party_roles.each do |party_role| %>
      <tr>
        <td><%= party_role.party_id %></td>
        <td><%= party_role.role_type_id %></td>
        <td><%= link_to 'Show', party_role %></td>
        <td><%= link_to 'Edit', edit_party_role_path(party_role) %></td>
        <td><%= link_to 'Destroy', party_role, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Party Role', new_party_role_path %>
